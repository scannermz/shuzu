package shuzu;

public class Text4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] a = {1,2,5,6,8,4,3,2,7};
		int max=a[0];
		int min=a[0];
		for(int i=1;i<a.length;i++)
		{
			if(a[i]>max)
			{
				max = a[i];
			}
		}
		for(int i=1;i<a.length;i++)
		{
			if(a[i]<min)
			{
				min = a[i];
			}
		}
		System.out.println(max+" "+min);
	}
