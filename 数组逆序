package shuzu;

public class Text5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] a = {234,3,34,25,3};
//		for(int i=a.length-1;i>=0;i--)
//		{
//			System.out.println(a[i]);
//		}
		for(int i=0;i<a.length/2;i++)
		{
			int temp = a[i];
			a[i]=a[a.length-1-i];
			a[a.length-1-i]=temp;
		}
		for(int i=0;i<a.length;i++)
		{
			System.out.println(a[i]);
		}
	}

}
