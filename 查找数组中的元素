package shuzu;

public class Text7 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int []a = {1,2,3,4,5,5,1,2,78,};
		int b = 9;
		int index=-1;
		for(int i=0;i<a.length;i++)
		{
			if(a[i]==b)
			{
				index=i;
				break;
			}
		}
		System.out.println(index);
	}

}
